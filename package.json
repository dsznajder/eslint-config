{
  "name": "eslint-config-dsznajder",
  "version": "0.1.12",
  "author": " Damian Sznajder <damsznajder@gmail.com>",
  "license": "MIT",
  "description": "Opinionated eslint config",
  "homepage": "https://github.com/dsznajder/eslint-config-dsznajder#readme",
  "keywords": [
    "eslint",
    "config",
    "prettier",
    "jest",
    "react-native"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "bugs": {
    "url": "https://github.com/dsznajder/eslint-config-dsznajder/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsznajder/eslint-config-dsznajder.git"
  },
  "main": "src/index.js",
  "scripts": {
    "release": "release-it --ci",
    "test": "eslint --ext '.js,.ts,.tsx' tests/**"
  },
  "dependencies": {
    "@babel/core": "^7.17.0",
    "@babel/eslint-parser": "^7.17.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.5.1"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^4.1.0",
    "husky": "^7.0.4",
    "release-it": "^14.12.4",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "eslint": "^8",
    "prettier": "^2"
  },
  "prettier": {
    "bracketSpacing": true,
    "bracketSameLine": false,
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  }
}
